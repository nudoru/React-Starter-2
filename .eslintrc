{
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "comma-dangle" : 1,
    "brace-style": 1,
    "camelcase": 1,
    "no-new-wrappers": 1,
    "no-floating-decimal": 1,
    "no-extra-bind": 1,
    "curly": 1,
    "default-case": 1,
    "dot-notation": 1,
    "eqeqeq": 1,
    "react-native/no-unused-styles": 2,
    "react-native/split-platform-components": 2
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "0.15.2"
    }
  },
}